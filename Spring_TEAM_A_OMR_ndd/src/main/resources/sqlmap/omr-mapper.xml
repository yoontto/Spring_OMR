<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="omr.mapper">
  <select id="id_check" resultType="int">
    select 1 from dual
  </select>
  
  <select id="listQuestion" resultType="test.TestVO">
  	select * from teama_test_${test_name}
  </select>
  
  <select id="searchQuestion" resultType="test.TestVO">
   	select * from teama_test_${test_name} where test_num = ${test_num}

  </select>
</mapper>